# Good Eggs MCP Server Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Your Good Eggs account credentials
GOOD_EGGS_USERNAME=your-email@example.com
GOOD_EGGS_PASSWORD=your-password

# Optional: Run browser in headless mode (default: true)
# Set to false to see the browser window during operations
HEADLESS=true

# Optional: Timeout for browser operations in milliseconds (default: 30000)
TIMEOUT=30000
